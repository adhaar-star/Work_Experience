@import url('https://fonts.googleapis.com/css?family=Roboto');
body,
html {
    height: 100%;
    font-family: 'Roboto', sans-serif;
    background: #F3F3F4;
}

textarea {
    resize: none;
}

.modal {
    text-align: center;
    padding: 0!important;
}

.modal:before {
    content: '';
    display: inline-block;
    height: 100%;
    vertical-align: middle;
    margin-right: -4px;
}

.modal-dialog {
    display: inline-block;
    text-align: left;
    vertical-align: middle;
}

a:hover {
    text-decoration: none;
}

/*----Login Css Start----*/

.login {
    background: url(../images/loging_bg_1.jpg);
    background-size: cover;
}

.app-brand img {
    max-width: 120px;
}

.app {
    min-height: 100vh;
}

/*.app.app-default {
    background: rgb(0,0,0, 0.7);
}*/

.app-body {
    left: 50%;
    position: absolute;
    top: 0;
    width: 100%;
    transform: translateX(-50%) translateY(-50%);
    -moz-transform: translateX(-50%) translateY(-50%);
    -webkit-transform: translateX(-50%) translateY(-50%);
    -o-transform: translateX(-50%) translateY(-50%);
    max-width: 450px;
    width: 100%;
    -webkit-box-shadow: 0px 1px 5px 0px rgba(50, 50, 50, 0.75);
    -moz-box-shadow: 0px 1px 5px 0px rgba(50, 50, 50, 0.75);
    box-shadow: 0px 1px 5px 0px rgba(50, 50, 50, 0.75);
    margin-top: 200px;
}

body .app-container.admin-login {
    min-height: 100vh !important;
}

.admin-login .app-body .app-block {
    transition: all 0.2s ease;
    background-color: #FFF;
    border-radius: 2px;
    position: relative;
}

.admin-login .app-body .app-block .app-form {
    padding: 2rem;
    position: relative;
    -ms-flex: 1;
    flex: 1;
}

.admin-login .app-body .app-block .app-form .form-header {
    text-align: center;
    font-size: 2em;
    font-weight: 200;
    color: #444;
    margin-bottom: 25px;
}

.admin-login .app-body .app-block .app-form form {
    min-width: 280px;
}

.input-group {
    border: 1px solid #c8d1d3;
    border-radius: 2px;
    margin-bottom: 20px;
    width: 100%;
}

.input-group .input-group-addon {
    font-size: 17px;
    width: auto;
    border: 0;
    background-color: transparent;
    width: 40px;
    text-align: center;
    color: #935bbd;
}

.input-group .input-group-addon + .form-control {
    border-left: 1px solid #dfe6e8 !important;
    border: 0px;
    background: none;
    box-shadow: inset 0 0px 0px rgba(0, 0, 0, .075);
}

.admin-login .app-body .app-block .app-form form .btn-submit {
    width: 100%;
    font-family: 'Open Sans', sans-serif;
}

.btn.btn-success {
    border-color: #ab8ce4;
    border-bottom-color: #ab8ce4;
    border-bottom-color: #ab8ce4;
    background: #ab8ce4;

}

.btn.btn-success:active:focus {
    background-color: #5a2d7c;
    border-color: #5a2d7c;
}

.admin-login label.error {
    top: 36px !important;
    position: absolute;
    left: 0;
}

.admin-login .btn-submit {
    padding: 10px 12px;
    font-size: 18px;
    border-radius: 0;
}

label.error {
    color: red;
    font-size: 12px;

    width: 100%;
    font-weight: normal;
}

#date_time-error {
    position: absolute;
    left: 0;
    top: 39px;
}

.alert-warning .fa {
    font-size: 29px;
    vertical-align: middle;
    margin-right: 10px;
}

.alert {
    font-size: 16px;
}

.btn.btn-success:hover {
    border-color: #9369de;
    border-bottom-color: rgb(171, 140, 228);
    border-bottom-color: #9369de;
    border-bottom-color: #9369de;
    background: #9369de;
}

.save_btn {
    font-size: 19px;
}

/*----Login Css End----*/

